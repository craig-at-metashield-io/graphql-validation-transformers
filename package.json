{
   "name": "graphql-validation-transformers",
   "version": "1.0.8",
   "description": "A set of field-level validators for Amplify GraphQL",
   "keywords": [
      "AWS",
      "Amplify",
      "GraphQL",
      "Validation",
      "Field"
   ],
   "author": "craig@metashield.io",
   "license": "MIT",
   "workspaces": [
      "packages/*"
   ],
   "scripts": {
      "build": "npm run build --workspaces",
      "test": "jest",
      "prepublishOnly": "npm run build && npm run test",
      "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
   },
   "devDependencies": {
      "@types/node": "^20.9.2",
      "@typescript-eslint/eslint-plugin": "^6.11.0",
      "@typescript-eslint/parser": "^6.11.0",
      "eslint": "^8.54.0",
      "prettier": "^3.1.0",
      "ts-node": "^10.9.1",
      "@types/jest": "^29.5.1",
      "jest": "^29.5.0",
      "ts-jest": "^29.1.0"
   },
   "dependencies": {
      "@aws-amplify/graphql-model-transformer": "^1.4.0",
      "@aws-amplify/amplify-cli-core": "^4.2.7",
      "@aws-amplify/graphql-transformer-core": "^1.4.0",
      "@aws-amplify/graphql-transformer-interfaces": "^3.3.0",
      "graphql": "^15.8.0",
      "graphql-mapping-template": "^4.20.8",
      "graphql-transformer-common": "^4.24.5",
      "typescript": "^4.9.5",
      "@graphql-validation-transformers/common": "^1.0.8",
      "@graphql-validation-transformers/string": "^1.0.8",
      "@graphql-validation-transformers/number": "^1.0.8"
